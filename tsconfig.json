{
    "extends": "./src/maskbook/packages/config/tsconfig.json",
    "references": [
        {
            "path": "./src/maskbook/packages/polyfills/tsconfig.json"
        },
        {
            "path": "./src/maskbook/packages/mask/tsconfig.json"
        },
        {
            "path": "./src/maskbook/packages/injected-script/main/tsconfig.json"
        },
        {
            "path": "./src/maskbook/packages/mask-sdk/main/tsconfig.json"
        },
        {
            "path": "./src/maskbook/packages/mask-sdk/public-api/tsconfig.json"
        },
        {
            "path": "./src/maskbook/packages/plugins/tsconfig.json"
        }
    ],
    "compilerOptions": {
        "noEmit": true,
        "composite": true,
        "esModuleInterop": true,
        "jsx": "preserve",
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"],
            "@masknet/backup-format": ["./src/maskbook/packages/backup-format/src"],
            "@masknet/base": ["./src/maskbook/packages/base/src"],
            "@masknet/encryption": ["./src/maskbook/packages/encryption/src"],
            "@masknet/flags": ["./src/maskbook/packages/flags/src"],
            "@masknet/gun-utils": ["./src/maskbook/packages/gun-utils/src"],
            "@masknet/injected-script": ["./src/maskbook/packages/injected-script/sdk"],
            "@masknet/sdk": ["./src/maskbook/packages/mask-sdk/server"],
            "@masknet/sdk/shared": ["./src/maskbook/packages/mask-sdk/shared"],
            "@masknet/plugin-infra/dom": ["./src/maskbook/packages/plugin-infra/src/entry-dom.ts"],
            "@masknet/plugin-infra/dom/context": ["./src/maskbook/packages/plugin-infra/src/dom/context.ts"],
            "@masknet/plugin-infra/background-worker": [
                "./src/maskbook/packages/plugin-infra/src/entry-background-worker.ts"
            ],
            "@masknet/plugin-infra/extension-page": [
                "./src/maskbook/packages/plugin-infra/src/entry-extension-page.ts"
            ],
            "@masknet/plugin-infra/content-script/context": [
                "./src/maskbook/packages/plugin-infra/src/site-adaptor/context.ts"
            ],
            "@masknet/plugin-infra/content-script": [
                "./src/maskbook/packages/plugin-infra/src/entry-content-script.ts"
            ],
            "@masknet/plugin-infra": ["./src/maskbook/packages/plugin-infra/src/entry.ts"],
            "@masknet/global-types/*": ["./src/maskbook/packages/polyfills/types/*"],
            "@masknet/public-api": ["./src/maskbook/packages/public-api/src"],
            "@masknet/sandboxed-plugin-runtime": [
                "./src/maskbook/packages/sandboxed-plugin-runtime/src/runtime/export.ts"
            ],
            "@masknet/sandboxed-plugin-runtime/background": [
                "./src/maskbook/packages/sandboxed-plugin-runtime/src/background"
            ],
            "@masknet/sandboxed-plugin-runtime/site-adaptor": [
                "./src/maskbook/packages/sandboxed-plugin-runtime/src/site-adaptor"
            ],
            "@masknet/sandboxed-plugin-runtime/extension-page": [
                "./src/maskbook/packages/sandboxed-plugin-runtime/src/extension-page"
            ],
            "@masknet/shared": ["./src/maskbook/packages/shared/src"],
            "@masknet/shared-base": ["./src/maskbook/packages/shared-base/src/index.ts"],
            "@masknet/shared-base-ui": ["./src/maskbook/packages/shared-base-ui/src"],
            "@masknet/theme": ["./src/maskbook/packages/theme/src"],
            "@masknet/typed-message": ["./src/maskbook/packages/typed-message/base"],
            "@masknet/typed-message-react": ["./src/maskbook/packages/typed-message/react"],
            "@masknet/types": ["./src/maskbook/packages/types/src"],
            "@masknet/web3-helpers": ["./src/maskbook/packages/web3-helpers/src"],
            "@masknet/web3-hooks-base": ["./src/maskbook/packages/web3-hooks/base/src"],
            "@masknet/web3-hooks-evm": ["./src/maskbook/packages/web3-hooks/evm/src"],
            "@masknet/web3-providers": ["./src/maskbook/packages/web3-providers/src/entry.ts"],
            "@masknet/web3-providers/helpers": ["./src/maskbook/packages/web3-providers/src/entry-helpers.ts"],
            "@masknet/web3-providers/types": ["./src/maskbook/packages/web3-providers/src/entry-types.ts"],
            "@masknet/web3-shared-base": ["./src/maskbook/packages/web3-shared/base/src"],
            "@masknet/web3-shared-evm": ["./src/maskbook/packages/web3-shared/evm/src"],
            "@masknet/web3-shared-flow": ["./src/maskbook/packages/web3-shared/flow/src"],
            "@masknet/web3-shared-solana": ["./src/maskbook/packages/web3-shared/solana/src"],
            "@masknet/web3-telemetry": ["./src/maskbook/packages/web3-telemetry/src/entry.ts"],
            "@masknet/web3-telemetry/helpers": ["./src/maskbook/packages/web3-telemetry/src/entry-helpers.ts"],
            "@masknet/web3-telemetry/types": ["./src/maskbook/packages/web3-telemetry/src/entry-types.ts"],
            "@masknet/plugin-debugger": ["./src/maskbook/packages/plugins/Debugger/src"],
            "@masknet/plugin-file-service": ["./src/maskbook/packages/plugins/FileService/src"],
            "@masknet/plugin-rss3": ["./src/maskbook/packages/plugins/RSS3/src"],
            "@masknet/plugin-template": ["./src/maskbook/packages/plugins/template/src"],
            "@masknet/plugin-cyberconnect": ["./src/maskbook/packages/plugins/CyberConnect/src"],
            "@masknet/plugin-go-plus-security": ["./src/maskbook/packages/plugins/GoPlusSecurity/src"],
            "@masknet/plugin-cross-chain-bridge": ["./src/maskbook/packages/plugins/CrossChainBridge/src"],
            "@masknet/plugin-web3-profile": ["./src/maskbook/packages/plugins/Web3Profile/src"],
            "@masknet/plugin-handle": ["./src/maskbook/packages/plugins/Handle/src"],
            "@masknet/plugin-approval": ["./src/maskbook/packages/plugins/Approval/src"],
            "@masknet/plugin-scam-warning": ["./src/maskbook/packages/plugins/ScamWarning/src"],
            "@masknet/plugin-gitcoin": ["./src/maskbook/packages/plugins/Gitcoin/src"],
            "@masknet/plugin-avatar": ["./src/maskbook/packages/plugins/Avatar/src"],
            "@masknet/plugin-redpacket": ["./src/maskbook/packages/plugins/RedPacket/src"],
            "@masknet/plugin-maskbox": ["./src/maskbook/packages/plugins/MaskBox/src"],
            "@masknet/plugin-smart-pay": ["./src/maskbook/packages/plugins/SmartPay/src"],
            "@masknet/plugin-transak": ["./src/maskbook/packages/plugins/Transak/src"],
            "@masknet/plugin-vcent": ["./src/maskbook/packages/plugins/VCent/src"],
            "@masknet/plugin-collectible": ["./src/maskbook/packages/plugins/Collectible/src"],
            "@masknet/plugin-claim": ["./src/maskbook/packages/plugins/Claim/src"],
            "@masknet/plugin-artblocks": ["./src/maskbook/packages/plugins/ArtBlocks/src"],
            "@masknet/plugin-pets": ["./src/maskbook/packages/plugins/Pets/src"],
            "@masknet/plugin-tips": ["./src/maskbook/packages/plugins/Tips/src"],
            "@masknet/plugin-nextid": ["./src/maskbook/packages/plugins/NextID/src"],
            "@masknet/plugin-trader": ["./src/maskbook/packages/plugins/Trader/src"],
            "@masknet/plugin-profilecard": ["./src/maskbook/packages/plugins/ProfileCard/src"],
            "@masknet/plugin-savings": ["./src/maskbook/packages/plugins/Savings/src"],
            "@masknet/plugin-snapshot": ["./src/maskbook/packages/plugins/Snapshot/src"],
            "@masknet/plugin-switch-logo": ["./src/maskbook/packages/plugins/SwitchLogo/src"],
            "@masknet/plugin-calendar": ["./src/maskbook/packages/plugins/Calendar/src"]
        }
    },
    "include": ["globals.d.ts", "next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules", "src/maskbook"],
    "ts-node": {
        "compilerOptions": {
            "noEmit": false
        },
        "esm": true,
        "swc": true
    }
}
