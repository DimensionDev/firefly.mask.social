{
  "locales": [
    "en",
    "qaa"
  ],
  "rootDir": ".",
  "pseudoLocale": "qaa",
  "fallbackLocales": {
    "qaa": "en",
    "default": "en"
  },
  "sourceLocale": "en",
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src/app/**",
        "src/components/**",
        "src/helpers/**",
        "src/hooks/**",
        "src/providers/**"
      ],
      "exclude": [
        "src/maskbook/**"
      ]
    }
  ],
  "format": "po",
  "formatOptions": {
    "origins": true,
    "lineNumbers": false
  },
  "orderBy": "messageId"
}
