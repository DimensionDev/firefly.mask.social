{
    "buildCommand": "./scripts/setup-vercel.sh",
    "headers": [
        {
            "source": "/.well-known/apple-app-site-association",
            "headers": [
                {
                    "key": "Content-type",
                    "value": "application/json"
                }
            ]
        }
    ],
    "redirects": [],
    "rewrites": [
        {
            "source": "/",
            "has": [
                {
                    "type": "host",
                    "value": "cz.firefly.social"
                }
            ],
            "destination": "/activity/cz"
        }
    ]
}
